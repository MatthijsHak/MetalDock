npts 54 54 54                        # num.grid points in xyz
parameter_file ad4_parameters_HD.dat # force field default parameter file
gridfld clean_1jzf.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A Ru N                  # ligand atom types
receptor clean_1jzf.pdbqt            # macromolecule
gridcenter 18.85000 10.52900 17.68600 # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map clean_1jzf.A.map                 # atom-specific affinity map
map clean_1jzf.Ru.map                # atom-specific affinity map
map clean_1jzf.N.map                 # atom-specific affinity map
elecmap clean_1jzf.e.map             # electrostatic potential map
dsolvmap clean_1jzf.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
nbp_r_eps 2.0   5.0 12 6 OA Ru
nbp_r_eps 2.0   5.0 12 6 SA Ru
nbp_r_eps 1.7   5.0 12 6 HD Ru
nbp_r_eps 2.0   5.0 12 6 NA Ru
nbp_r_eps 2.0   5.0 12 6  N Ru
