npts 54 54 54                        # num.grid points in xyz
parameter_file ad4_parameters_HD.dat # force field default parameter file
gridfld clean_3m0b.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A Ru C OA N             # ligand atom types
receptor clean_3m0b.pdbqt            # macromolecule
gridcenter -0.80400 -23.15200 -4.02700 # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map clean_3m0b.A.map                 # atom-specific affinity map
map clean_3m0b.Ru.map                # atom-specific affinity map
map clean_3m0b.C.map                 # atom-specific affinity map
map clean_3m0b.OA.map                # atom-specific affinity map
map clean_3m0b.N.map                 # atom-specific affinity map
elecmap clean_3m0b.e.map             # electrostatic potential map
dsolvmap clean_3m0b.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
nbp_r_eps 2.0   5.0 12 6 OA Ru
nbp_r_eps 2.0   5.0 12 6 SA Ru
nbp_r_eps 1.7   5.0 12 6 HD Ru
nbp_r_eps 2.0   5.0 12 6 NA Ru
nbp_r_eps 2.0   5.0 12 6  N Ru
