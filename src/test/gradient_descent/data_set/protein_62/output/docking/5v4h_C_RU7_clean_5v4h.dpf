autodock_parameter_version 4.2       # used by autodock to validate parameter set
outlev 1                             # diagnostic output level
parameter_file ad4_parameters_HD.dat    # parameter library filename
intelec                              # calculate internal electrostatics
seed pid time                        # seeds for random generator
ligand_types Ru C Cl    # atoms types in ligand
fld clean_5v4h.maps.fld              # grid_data_file
intnbp_r_eps 2.0   5.0 12 6 OA Ru
intnbp_r_eps 2.0   5.0 12 6 SA Ru
intnbp_r_eps 1.7   5.0 12 6 HD Ru
intnbp_r_eps 2.0   5.0 12 6 NA Ru
intnbp_r_eps 2.0   5.0 12 6  N Ru
map clean_5v4h.Ru.map                 # atom-specific affinity map
map clean_5v4h.C.map                 # atom-specific affinity map
map clean_5v4h.Cl.map                 # atom-specific affinity map
elecmap clean_5v4h.e.map             # electrostatics map
desolvmap clean_5v4h.d.map           # desolvation map

# Unbound Ligand Parameters
unbound_energy -3885.49                      # set the energy of the unbound state
move 5v4h_C_RU7.pdbqt                # small molecule
about 8.12800 -10.20700 -4.55200            # small molecule center

# SA Parameters
tstep 2.0
linear_schedule                      # linear_schedule or geometric_schedule
rt0 500                              # initial annealing temperature (absolute tmperature multiplied by gas constant
rtrf 0.9                             # annealing temperature reductin factor < 1 cools > 1 heats system
runs 50                              # number of docking runs
cycles 50                            # number of temperature reduction cycles
accs 30000                           # maximum number of accepted steps per cycle
rejs 30000                           # maximum number of rejected steps per cycle
select m                             # m selects the minimum state, 1 selects the last state during each cycle
trnrf 1.0                            # per cycle reduction factor for translation steps
quarf 1.0                            # per cycle reduction factor for orientation steps
dihrf 1.0                            # per cycle reduction factor for torsional dihedral steps
# Activate SA
simanneal 10                         # run this many SA docking
# Perform Analysis
rmsref 5v4h_C_RU7.pdbqt              # RMSD will be calculated with this file
rmsmode atype                        # Method to calculate the RMSD
rmstol 2.0                           # RMSD tolerance
analysis                             # perforem a ranked cluster analysis
