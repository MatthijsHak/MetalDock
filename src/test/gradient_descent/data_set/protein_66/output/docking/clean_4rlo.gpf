npts 54 54 54                        # num.grid points in xyz
parameter_file ad4_parameters_HD.dat # force field default parameter file
gridfld clean_4rlo.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A Ru C F NA OA N S HD   # ligand atom types
receptor clean_4rlo.pdbqt            # macromolecule
gridcenter 82.16100 1.42300 6.47500  # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map clean_4rlo.A.map                 # atom-specific affinity map
map clean_4rlo.Ru.map                # atom-specific affinity map
map clean_4rlo.C.map                 # atom-specific affinity map
map clean_4rlo.F.map                 # atom-specific affinity map
map clean_4rlo.NA.map                # atom-specific affinity map
map clean_4rlo.OA.map                # atom-specific affinity map
map clean_4rlo.N.map                 # atom-specific affinity map
map clean_4rlo.S.map                 # atom-specific affinity map
map clean_4rlo.HD.map                # atom-specific affinity map
elecmap clean_4rlo.e.map             # electrostatic potential map
dsolvmap clean_4rlo.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
nbp_r_eps 2.0   5.0 12 6 OA Ru
nbp_r_eps 2.0   5.0 12 6 SA Ru
nbp_r_eps 1.7   5.0 12 6 HD Ru
nbp_r_eps 2.0   5.0 12 6 NA Ru
nbp_r_eps 2.0   5.0 12 6  N Ru
