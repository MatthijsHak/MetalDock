npts 50 50 50                        # num.grid points in xyz
parameter_file ad4_parameters_HD.dat # force field default parameter file
gridfld clean_3x32.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD OA N           # receptor atom types
ligand_types Ru C S OA N             # ligand atom types
receptor clean_3x32.pdbqt            # macromolecule
gridcenter 19.33 24.55 -11.98        # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map clean_3x32.Ru.map                # atom-specific affinity map
map clean_3x32.C.map                 # atom-specific affinity map
map clean_3x32.S.map                 # atom-specific affinity map
map clean_3x32.OA.map                # atom-specific affinity map
map clean_3x32.N.map                 # atom-specific affinity map
elecmap clean_3x32.e.map             # electrostatic potential map
dsolvmap clean_3x32.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
nbp_r_eps 2.0000  10.0000    12 6 OA Ru
nbp_r_eps 2.0000  10.0000    12 6 SA Ru
nbp_r_eps 2.0000  10.0000    12 6 HD Ru
nbp_r_eps 2.0000  10.0000    12 6 NA Ru
nbp_r_eps 2.0000  10.0000    12 6  N Ru
