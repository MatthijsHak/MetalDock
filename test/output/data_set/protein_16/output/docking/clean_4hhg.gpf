npts 32 42 36                        # num.grid points in xyz
parameter_file ad4_parameters_HD.dat # force field default parameter file
gridfld clean_4hhg.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A Ru C NA N             # ligand atom types
receptor clean_4hhg.pdbqt            # macromolecule
gridcenter 10.1 -19. -14.            # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map clean_4hhg.A.map                 # atom-specific affinity map
map clean_4hhg.Ru.map                # atom-specific affinity map
map clean_4hhg.C.map                 # atom-specific affinity map
map clean_4hhg.NA.map                # atom-specific affinity map
map clean_4hhg.N.map                 # atom-specific affinity map
elecmap clean_4hhg.e.map             # electrostatic potential map
dsolvmap clean_4hhg.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
nbp_r_eps 1.0448  19.0741    12 6 OA Ru
nbp_r_eps 1.7558  8.2304    12 6 SA Ru
nbp_r_eps 1.9146  0.8403    12 6 HD Ru
nbp_r_eps 2.9471  12.5224    12 6 NA Ru
nbp_r_eps 2.2131  11.3151    12 6  N Ru
